{
    "name": "UNet Testing",
    "version": "0.1.1",
    "title": "UNet Testing",
    "description": "WIPP plugin to test UNet model from UFreiburg",
    "author": "Vishakha Goyal (vishakha.goyal@nih.gov)",
    "institution": "National Center for Advancing Translational Sciences, National Institutes of Health",
    "repository": "https://github.com/labshare/polus-plugins",
    "website": "https://ncats.nih.gov/preclinical/core/informatics",
    "citation": "",
    "containerId": "labshare/polus-unet-testing-plugin:0.1.1",
    "inputs": [
      {
        "name": "filename",
        "type": "string",
        "description": "Weights file name for testing.",
        "required": true
      },
      {
        "name": "inpDir",
        "type": "collection",
        "description": "Input image collection to be processed by this plugin.",
        "required": true
      },
      {
        "name": "pixelsize",
        "type": "number",
        "description": "Input image pixel size.",
